{
    "debug": false,
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "/var/log/trade/cachecenter",
        "flag": "fatal,error,warn,info,debug,trace",
        "num": 10
    },
    "alert": {
        "host": "cache",
        "addr": "127.0.0.1:4444"
    },
    "svr": {
        "bind": "tcp@0.0.0.0:7801",
        "max_pkg_size": 10240
    },
    "deals_svr": {
        "bind": "tcp@0.0.0.0:7802",
        "max_pkg_size": 10240
    },
     "state_svr": {
        "bind": "tcp@0.0.0.0:7803",
        "max_pkg_size": 10240
    },
    "matchengine": {
        "name": "matchengine",
        "addr": [
            "tcp@127.0.0.1:7316"
        ],
        "max_pkg_size": 2000000
    },
    "marketprice": {
        "name": "marketprice",
        "addr": [
            "tcp@127.0.0.1:7416"
        ],
        "max_pkg_size": 2000000
    },
    "backend_timeout": 5.0,
    "sub_depth_interval": 1,
    "sub_status_interval": 0.5,
    "sub_deals_interval": 0.5,
    "depth_limit_max": 50,
    "cache_timeout": 500,
    "depth_merge": ["0", "0.00000001", "0.0000001", "0.000001", "0.00001", "0.0001", "0.001", "0.01", "0.1", "1", "10", "100"]
}

