{
    "debug": true,
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "/var/log/trade/accesshttp",
        "flag": "fatal,error,warn,info,debug,trace",
        "num": 10
    },
    "alert": {
        "host": "accesshttp",
        "addr": "127.0.0.1:4444"
    },
    "svr": {
        "bind": [
            "tcp@0.0.0.0:8080"
        ],
        "max_pkg_size": 102400
    },
    "worker_num": 4,
    "timeout": 1.0,
    "matchengine": {
        "name": "matchengine",
        "addr": [
            "tcp@127.0.0.1:7316"
        ],
        "max_pkg_size": 2000000
    },
    "marketprice": {
        "name": "marketprice",
        "addr": [
            "tcp@127.0.0.1:7416"
        ],
        "max_pkg_size": 2000000
    },
    "readhistory": {
        "name": "readhistory",
        "addr": [
            "tcp@127.0.0.1:7516"
        ],
        "max_pkg_size": 2000000
    },
    "monitorcenter": {
        "name": "monitorcenter",
        "addr": [
            "tcp@127.0.0.1:5555"
        ],
        "max_pkg_size": 2000000
    },
    "cache": {
        "name": "cache",
        "addr": [
            "tcp@127.0.0.1:7801"
        ],
        "max_pkg_size": 2000000,
        "heartbeat_timeout": 30
    },
    "depth_limit": [5, 10, 20, 50],
    "depth_merge": ["0", "0.00000001", "0.0000001", "0.000001", "0.00001", "0.0001", "0.001", "0.01", "0.1", "1", "10", "100"]
}
