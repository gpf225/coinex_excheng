{
    "debug": true,
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "/var/log/trade/cache",
        "flag": "fatal,error,warn,info,debug,trace",
        "num": 10
    },
    "alert": {
        "host": "cache",
        "addr": "127.0.0.1:4444"
    },
    "svr": {
        "bind": "tcp@0.0.0.0:7801",
        "max_pkg_size": 10240
    },
    "matchengine": {
        "name": "matchengine",
        "addr": [
            "tcp@127.0.0.1:7316"
        ],
        "max_pkg_size": 2000000
    },
    "backend_timeout": 5.0,
    "sub_depth_interval": 0.5,
    "sub_kline_interval": 0.5,
    "sub_status_interval": 0.5,
    "cache_timeout": 1000,
    "depth_limit_max": 50,
    "deal_default_limit": 1000
}
